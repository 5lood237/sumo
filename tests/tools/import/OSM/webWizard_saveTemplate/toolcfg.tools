<configuration>
    <remote value="False" type="bool" help="In remote mode, SUMO GUI will not be automatically opened instead a zip file will be generated."/>
    <osm-file value="osm_bbox.osm.xml" type="string" help="use input file from path."/>
    <test-output value="None" type="string" help="Run with pre-defined options on file 'osm_bbox.osm.xml' and write output to the given directory."/>
    <bbox value="None" type="string" help="bounding box to retrieve in geo coordinates west,south,east,north."/>
    <output value="None" type="string" help="Write output to the given folder rather than creating a name based on the timestamp"/>
    <address value="" type="string" help="Address for the Websocket."/>
    <port value="8010" type="int" help="Port for the Websocket. Please edit script.js when using an other port than 8010."/>
    <verbose value="False" type="bool" help="tell me what you are doing"/>
    <begin value="0" type="time" help="Defines the begin time for the scenario."/>
    <end value="900" type="time" help="Defines the end time for the scenario."/>
    <netconvert-options value="None" type="string" help="additional comma-separated options for netconvert"/>
    <demand value="passenger:6f5,bicycle:2f2,pedestrian:4,ship:1f40" type="string" help="Traffic demand definition for non-interactive mode."/>
</configuration>
